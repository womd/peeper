<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".activity.HFPingActivity">

    <TextView
            android:text="@string/Samples"
            android:layout_width="139dp"
            android:layout_height="18dp"
            android:id="@+id/textViewLabelFrequency2"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="12dp" android:layout_marginStart="12dp"
            app:layout_constraintTop_toBottomOf="@+id/seekBarFrequency" android:layout_marginTop="12dp"/>
    <SeekBar
            android:layout_width="387dp"
            android:layout_height="13dp" android:id="@+id/seekBarSamples"
            app:layout_constraintTop_toBottomOf="@+id/textViewSamples" android:layout_marginTop="12dp"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"/>
    <Button
            android:text="Button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/button"
            android:onClick="testButtonClick"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="12dp" android:layout_marginStart="12dp"
            app:layout_constraintTop_toBottomOf="@+id/seekBarSamples" android:layout_marginTop="20dp"/>
    <SeekBar
            android:layout_width="381dp"
            android:layout_height="14dp" android:id="@+id/seekBarFrequency"
            app:layout_constraintTop_toBottomOf="@+id/textViewFrequency" android:layout_marginTop="8dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"/>
    <TextView
            android:layout_width="177dp"
            android:layout_height="18dp" android:id="@+id/textViewFrequency"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="16dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="16dp"
            android:layout_marginRight="16dp"/>
    <TextView
            android:layout_width="179dp"
            android:layout_height="18dp" android:id="@+id/textViewSamples"
            app:layout_constraintTop_toBottomOf="@+id/seekBarFrequency"
            android:layout_marginTop="12dp" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="16dp"
            android:layout_marginRight="16dp"/>
    <TextView
            android:text="@string/Frequency"
            android:layout_width="139dp"
            android:layout_height="18dp" app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="16dp" android:id="@+id/textViewLabelFrequency" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="12dp" android:layout_marginStart="12dp"/>
</androidx.constraintlayout.widget.ConstraintLayout>